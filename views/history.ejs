<!DOCTYPE html>
<html>
<head>
	<title></title>
	<script type="text/javascript" src="/js/jquery.min.js"></script>
	
	<style type="text/css">
		ul{
			padding-left: 10px;
			padding-right: 10px;
			list-style-type: none;
			font-size: 120%;
		}

		ul :nth-child(odd){
			background: #eee;
		}

		li {
			padding-top: 5px;
			padding-bottom: 5px;
			word-wrap: break-word;
		}
	</style>
</head>
<body onload="timeFormat()">
	<ul class="msgs"></ul>
</body>
<script type="text/javascript" src="/js/postMsg.js"></script>
<script type="text/javascript">
	var $messages = $('.msgs');
	
	function timeFormat(){
		var msgs = '<%- JSON.stringify(msgs) %>';
		msgs = JSON.parse(msgs);

		for(var i in msgs){
			postMsg(msgs[i].name, msgs[i].msg, msgs[i].time);
		}
	}
</script>
</html>